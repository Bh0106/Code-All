#include<stdio.h>
#include<string.h>
int main()
{
    int i, j, n, num[10] = {0};
    char m[1000];
    scanf("%s", &m);
    for(i = 0; i < strlen(m); i++) {
        num[m[i] - '0']++;
    }
    for(i = 0; i < 10; i++) {
        if(num[i]!= 0) {
            printf("%d:%d\n", i, num[i]);
        }
    }
}
